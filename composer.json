{
    "name": "nick-kh/laravel-filament-cms",
    "description": "CMS build on laravel and filament",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "livewire/livewire": "^2.12",
        "filament/spatie-laravel-media-library-plugin": "^2.0",
        "filament/spatie-laravel-translatable-plugin": "^2.0",
        "nick-kh/laravel-filament-cms": "*"
    },
    "license": "MIT",

    "authors": [
        {
            "name": "Nick Kharadze",
            "email": "nikakharadze@yahoo.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "BossmanFilamentApp\\": "src/"

        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BossmanFilamentApp\\SiteBuilderServiceProvider"
            ],
            "aliases": {
                "FilamentTestPlugin": "BossmanFilamentApp"
            }
        }
    }
}
